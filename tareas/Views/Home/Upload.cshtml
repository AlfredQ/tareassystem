<link href="~/css/dropzone.css" rel="stylesheet"/>
<script src="~/Scripts/downloads/dropzone.js"></script>
<script>
    var Dropzone = require("dropzone");
</script> 
@{
    ViewBag.Title = "Upload";
}

<h2>Upload</h2>
<form action="/Home/subir" class="dropzone" method="post" enctype="multipart/form-data">
  <div class="fallback">
    <input name="file" type="file" multiple="multiple" />
    <input type="submit"  value="Subir"/>
  </div>
</form>